<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity StorageArray.
    -->
    <changeSet id="20180703100843-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="datastore_id"
                                 baseTableName="storage_array"
                                 constraintName="fk_storage_array_datastore_id"
                                 referencedColumnNames="id"
                                 referencedTableName="datastore"/>

        <addForeignKeyConstraint baseColumnNames="virtualvolume_id"
                                 baseTableName="storage_array"
                                 constraintName="fk_storage_array_virtualvolume_id"
                                 referencedColumnNames="id"
                                 referencedTableName="virtual_volume"/>

    </changeSet>
</databaseChangeLog>
